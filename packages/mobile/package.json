{
  "name": "kartonah-mobile",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "ENVFILE=.env react-native run-android --variant=debug",
    "android:staging": "ENVFILE=.env.staging react-native run-android --variant=stagingrelease",
    "android:production": "ENVFILE=.env.production react-native run-android --variant=productionrelease",
    "build:detox:android": "detox build --configuration android",
    "build:detox:ios": "detox build --configuration ios",
    "detox:android": "npm run build:detox:android && npm run test:detox:android",
    "detox:ios": "npm run build:detox:ios && npm run test:detox:ios",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "test:dev": "jest --watch",
    "test:detox:android": "detox test --configuration android",
    "test:detox:ios": "detox test --configuration ios",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "map":"adb reverse tcp:4000 tcp:4000",
    "extract": "i18next-scanner --config i18next-scanner.config.js --output /App/i18n/lang 'App/**/*.{ts,tsx}'",
    "release:android": "cd android && ENVFILE=.env.production ./gradlew bundleRelease",
    "release:android:apk:prod": "cd android && ENVFILE=.env.production ./gradlew assembleRelease",
    "release:android:apk:dev": "cd android && ENVFILE=.env.staging ./gradlew assembleRelease",
    "push:ios": "appcenter codepush release-react -a Kartonah/Kartonah-IOS -d Production -t 4.0.5  -m",
    "push:android": "appcenter codepush release-react -a Kartonah/Kartonah-ANDROID -d Production -t 4.0.6 -m",
    "push:ios:staging": "appcenter codepush release-react -a Kartonah/Kartonah-IOS -d Staging -t 4.0.5 -m",
    "push:android:staging": "appcenter codepush release-react -a Kartonah/Kartonah-ANDROID -d Staging -t 4.0.6 -m"
  },
